#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@rmp135/sql-ts/bin/sql-ts" "$@"
else 
  exec node  "$basedir/../@rmp135/sql-ts/bin/sql-ts" "$@"
fi

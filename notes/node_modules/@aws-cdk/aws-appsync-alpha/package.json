{
  "name": "@aws-cdk/aws-appsync-alpha",
  "version": "2.24.0-alpha.0",
  "private": false,
  "description": "The CDK Construct Library for AWS::AppSync",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "jsii": {
    "outdir": "dist",
    "targets": {
      "java": {
        "package": "software.amazon.awscdk.services.appsync.alpha",
        "maven": {
          "groupId": "software.amazon.awscdk",
          "artifactId": "appsync-alpha"
        }
      },
      "dotnet": {
        "namespace": "Amazon.CDK.AWS.AppSync.Alpha",
        "packageId": "Amazon.CDK.AWS.AppSync.Alpha",
        "iconUrl": "https://raw.githubusercontent.com/aws/aws-cdk/master/logo/default-256-dark.png"
      },
      "python": {
        "distName": "aws-cdk.aws-appsync-alpha",
        "module": "aws_cdk.aws_appsync_alpha",
        "classifiers": [
          "Framework :: AWS CDK",
          "Framework :: AWS CDK :: 2"
        ]
      },
      "go": {
        "moduleName": "github.com/aws/aws-cdk-go",
        "packageName": "awscdkappsyncalpha"
      }
    },
    "projectReferences": true,
    "metadata": {
      "jsii": {
        "rosetta": {
          "strict": true
        }
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aws/aws-cdk.git",
    "directory": "packages/individual-packages/aws-appsync"
  },
  "scripts": {
    "build": "cdk-build",
    "watch": "cdk-watch",
    "lint": "cdk-lint",
    "test": "cdk-test",
    "integ": "integ-runner",
    "pkglint": "pkglint -f",
    "package": "cdk-package",
    "awslint": "cdk-awslint",
    "build+test+package": "yarn build+test && yarn package",
    "build+test": "yarn build && yarn test",
    "compat": "cdk-compat",
    "rosetta:extract": "yarn --silent jsii-rosetta extract",
    "build+extract": "yarn build && yarn rosetta:extract",
    "build+test+extract": "yarn build+test && yarn rosetta:extract"
  },
  "cdk-build": {
    "env": {
      "AWSLINT_BASE_CONSTRUCT": true
    }
  },
  "keywords": [
    "aws",
    "cdk",
    "constructs",
    "appsync"
  ],
  "author": {
    "name": "Amazon Web Services",
    "url": "https://aws.amazon.com",
    "organization": true
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "@aws-cdk/cdk-build-tools": "2.24.0",
    "@aws-cdk/integ-runner": "2.24.0",
    "@aws-cdk/cfn2ts": "2.24.0",
    "@aws-cdk/pkglint": "2.24.0",
    "@types/jest": "^27.5.0",
    "jest": "^27.5.1",
    "aws-cdk-lib": "2.24.0",
    "constructs": "^10.0.0"
  },
  "dependencies": {},
  "homepage": "https://github.com/aws/aws-cdk",
  "peerDependencies": {
    "aws-cdk-lib": "^2.24.0",
    "constructs": "^10.0.0"
  },
  "engines": {
    "node": ">= 14.15.0"
  },
  "awslint": {
    "exclude": [
      "*:*"
    ]
  },
  "stability": "experimental",
  "maturity": "experimental",
  "awscdkio": {
    "announce": false
  },
  "publishConfig": {
    "tag": "latest"
  },
  "pkglint": {
    "exclude": [
      "naming/package-matches-directory",
      "assert/assert-dependency"
    ]
  }
}
